{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug KillFeed (ts-node)",
        "runtimeExecutable": "node",
        "runtimeArgs": [
          "--require",
          "ts-node/register"
        ],
        "args": [
          "${workspaceFolder}/src/Bot.ts"
        ],
        "cwd": "${workspaceFolder}",
        "envFile": "${workspaceFolder}/.env",
        "env": {
          "NODE_ENV": "development"
        },
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
  }
  